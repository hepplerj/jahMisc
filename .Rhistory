library(readr)
number_ticks <- function(n) {function(limits) pretty (limits, n)}
dat <- read.csv("/Users/jasonheppler/Dropbox/acad/data/scc-land/openspace.csv", header=T, sep=",")
p <- ggplot(dat, aes(x=Date, y=Acres, color=County)) + geom_point(shape=19) +
ggtitle("Open Spaces Acres in the Bay Area") +
geom_text(aes(label=ifelse(Acres > 8000, as.character(Name), '')), hjust=-0.05, size = 3.0) +
scale_x_continuous(breaks = number_ticks(10)) +
scale_color_brewer(palette = "Paired", name = "") +
theme_jah()
